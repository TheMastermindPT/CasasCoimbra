{
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jquery": true
  },
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    // "prettier/prettier": ["error", { "singleQuote": true }],
    "func-names": "off",
    "no-process-exit": "off",
    "linebreak-style": 0,
    "no-plusplus": "off",
    "no-unused-expressions": "off"
  }
}
